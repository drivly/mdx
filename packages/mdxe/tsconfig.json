{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "outDir": "dist",
    "declaration": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    ".next/types/**/*.ts",
    "/home/ubuntu/repos/mdx/examples/minimal/.next/types/**/*.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "/home/ubuntu/repos/mdx/examples/blog/.next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
