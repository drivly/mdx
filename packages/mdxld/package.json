{
  "name": "mdxld",
  "version": "0.1.0",
  "description": "Markdown & MDX Parser with YAML-LD Frontmatter Support and Component Separation",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ast": {
      "types": "./dist/ast.d.ts",
      "import": "./dist/ast.js"
    },
    "./parser": {
      "types": "./dist/parser.d.ts",
      "import": "./dist/parser.js"
    }
  },
  "scripts": {
    "build": "tsc && node build.js",
    "dev": "tsc --watch",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "markdown",
    "mdx",
    "yaml",
    "yaml-ld",
    "frontmatter",
    "linked-data",
    "ast"
  ],
  "author": "Drivly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/mdx"
  },
  "bugs": {
    "url": "https://github.com/drivly/mdx/issues"
  },
  "homepage": "https://mdxld.org",
  "dependencies": {
    "json5": "^2.2.3",
    "jsonld": "^8.3.2",
    "mdast-util-to-string": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.1.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jsonld": "^1.5.15",
    "@types/mdast": "^4.0.4",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  }
}
