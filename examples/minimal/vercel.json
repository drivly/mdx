{
  "framework": "nextjs",
  "buildCommand": "bash -c \"pnpm build\"",
  "devCommand": "bash -c \"pnpm dev\"",
  "installCommand": "bash -c \"pnpm install\"",
  "outputDirectory": ".next",
  "env": {
    "NEXT_OUTPUT": "standalone",
    "NEXT_DIST_DIR": ".next"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/$1" }
  ]
}
